package ncs.test1;

import java.util.Scanner;

public class Test1 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.print("부서별 평가 점수 모두 입력 : ");
		String score = sc.nextLine();
		String[] scoreArr = score.split(" ");
		
		int data[] = new int[5];
		int sum = 0;
		
		for(String a : scoreArr) {
			data = a;
		}
	}
}
